{
  "name": "crosscheck-widget",
  "version": "1.0.0",
  "description": "crosscheck-widget React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "start-storybook -p 6006 -s ./stories/data",
    "build-storybook": "build-storybook",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.19.1",
    "d3": "^5.7.0",
    "d3-array": "^2.0.3",
    "d3-scale-chromatic": "^1.3.3",
    "immutable": "^4.0.0-rc.12"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "babel-loader": "^8.0.4",
    "node-sass": "^7.0.0",
    "nwb": "0.21.x",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-json-view": "^1.19.1"
  },
  "author": "Dustin Arendt",
  "homepage": "https://github.com/pnnl/crosscheck",
  "license": "BSD",
  "repository": "https://github.com/pnnl/crosscheck",
  "keywords": [
    "react-component"
  ]
}
